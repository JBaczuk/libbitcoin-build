<buildgen language="en-us" >
    <templates>
        <template name="gsl.version.hpp"   generator="generate_version" />
        <template name="gsl.include.hpp"   generator="generate_include" />
        <!-- <template name="gsl.interface.swg" generator="generate_interface" /> -->
        <!-- <template name="gsl.bindings.sh"   generator="generate_bindings_sh" /> -->
        <!-- <template name="gsl.bindings.bat"  generator="generate_bindings_bat" /> -->
        <template name="gsl.autogen.sh"    generator="generate_autogen" />
        <template name="gsl.configure.ac"  generator="generate_configure" />
        <template name="gsl.Makefile.am"   generator="generate_automake" />
        <template name="gsl.package.pc.in" generator="generate_pkgconfig" />
        <template name="gsl.install.sh"    generator="generate_installer" />
        <template name="gsl.runner.sh"     generator="generate_runner" />
        <template name="gsl.travis.yml"    generator="generate_travis_yml" />
        <template name="gsl.build.cmd"     generator="generate_build_cmd" />
        <!-- <template name="gsl.build.ps1"     generator="generate_build_ps1" /> -->
        <template name="gsl.appveyor.yml"  generator="generate_appveyor_yml" />
        <!--
        <template name="gsl.gitignore" />
        <template name="gsl.packages.config" />
        <template name="gsl.vcxproj" />
        <template name="gsl.vcxproj.filters" />
        -->
    </templates>
    <repositories>
        <repository name="libbitcoin" />
        <repository name="libbitcoin-blockchain" />
        <repository name="libbitcoin-client" />
        <repository name="libbitcoin-consensus" />
        <repository name="libbitcoin-database" />
        <repository name="libbitcoin-explorer" />
        <repository name="libbitcoin-network" />
        <repository name="libbitcoin-node" />
        <repository name="libbitcoin-protocol" />
        <repository name="libbitcoin-server" />
    </repositories>
</buildgen>
